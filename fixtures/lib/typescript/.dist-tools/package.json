{
    "name": "@udx-worker/lint",
    "version": "1.0.0",
    "author": "UDX",
    "private": "true",
    "scripts": {
      "lint:app": "eslint ../api-service --ext .ts --config ./.eslintrc",
      "lint:utils": "eslint ../utils --ext .ts --config ./.eslintrc",
      "lint": "npm run lint:utils && npm run lint:api && npm run lint:trigger",
      "test:api": "npm test --prefix ../api-service",
      "test:trigger": "npm test --prefix ../pubsub-trigger",
      "test": "npm run test:api && npm run test:trigger"
    },
    "devDependencies": {
      "@typescript-eslint/eslint-plugin": "^5.61.0",
      "@typescript-eslint/parser": "^5.61.0",
      "eslint": "^8.44.0"
    }
  }